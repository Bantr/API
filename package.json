{
  "name": "ban-notifier",
  "version": "0.0.2",
  "description": "",
  "license": "MIT",
  "author": "",
  "scripts": {
    "build": "rimraf dist && nest build",
    "clean": "rimraf dist && rimraf node_modules",
    "db:migrate": "yarn typeorm migration:run",
    "db:migrate:gen": "yarn typeorm migration:generate -d src/migrations",
    "format": "prettier --write \"src/**/*.ts\"",
    "postinstall": "npm run build",
    "lint": "eslint . --ext .js,.ts",
    "start": "nest start",
    "start:debug": "nest start -d",
    "start:dev": "nest start -w",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@bantr/lib": "^1.9.0",
    "@hapi/joi": "^17.1.0",
    "@nestjs/cli": "^7.1.2",
    "@nestjs/common": "^7.0.9",
    "@nestjs/config": "^0.4.1",
    "@nestjs/core": "^7.0.9",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.8.0",
    "@nestjs/platform-socket.io": "^6.8.0",
    "@nestjs/swagger": "^4.5.3",
    "@nestjs/typeorm": "^6.2.0",
    "@nestjs/websockets": "^6.8.0",
    "@sentry/node": "5.15.4",
    "bcrypt": "^4.0.0",
    "bull": "^3.10.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "connect-flash": "^0.1.1",
    "connect-redis": "^4.0.3",
    "dotenv": "^8.1.0",
    "express-session": "^1.16.2",
    "morgan": "^1.9.1",
    "nestjs-redis": "^1.2.7",
    "passport": "^0.4.0",
    "passport-discord": "^0.1.3",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.5.0",
    "passport-steam": "^1.0.12",
    "pg": "^8.0.3",
    "querystring": "^0.2.0",
    "read-package-json": "^2.1.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/testing": "^7.0.9",
    "@types/connect-redis": "^0.0.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.11",
    "@types/hapi__joi": "^16.0.12",
    "@types/jest": "^25.2.1",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.13.2",
    "@types/socket.io": "^2.1.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.0",
    "tsc-watch": "^4.2.3",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.6.3"
  }
}
